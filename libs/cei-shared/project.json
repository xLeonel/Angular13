{
  "projectType": "library",
  "sourceRoot": "libs/cei-shared/src",
  "prefix": "caixa-eletronico",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:ng-packagr-lite",
      "outputs": ["dist/libs/cei-shared"],
      "options": {
        "project": "libs/cei-shared/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/cei-shared/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/cei-shared/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cei-shared"],
      "options": {
        "jestConfig": "libs/cei-shared/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/cei-shared/**/*.ts",
          "libs/cei-shared/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
